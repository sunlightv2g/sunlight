.gnb_bg {position:absolute;left:0;top:98px;width:100%;background-color:#fff;z-index:10;-webkit-box-shadow: 0px 3px 3px 0px rgba(170,170,170,0.6);-moz-box-shadow: 0px 3px 3px 0px rgba(170,170,170,0.6);
box-shadow: 0px 3px 3px 0px rgba(170,170,170,0.6);}
#gnb {position:relative;}
