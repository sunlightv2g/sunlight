@charset "utf-8";
@import url('reset.css');

#skipnavi {position:relative;width:100%;z-index:2;*height:0px;}
#skipnavi a {display:block;overflow:hidden;height:0px;width:0px;margin:-1px -1px -1px 0;*margin:-5px -1px -1px 0;font-size:0;line-height:0;text-align:center;}
#skipnavi:focus {overflow:hidden;height:100px;background:#0d2f51;font-size:12px;font-weight:500;line-height:140%;}
#skipnavi a:focus, #skipnavi a:hover, #skipnavi a:active {width:auto;height:20px;margin:0;padding:8px 0 0 15px;background:#0d2f51;font-size:12px;font-weight:500;line-height:140%;color:#fff;}

/***** layout *****/
#wrapper {position:relative;height:100%;min-width:1200px;min-height:600px;*min-height:600px}
#header {position:relative;width:100%;height:97px;border-bottom: 1px solid #e6e6e6;z-index:1000;background: url(../images/common/bg_header.png) repeat-x;}
#header.on {background:#fff url(../images/common/bg_header.png) repeat-x;position:fixed;-webkit-transition: all 0.7s ease; -moz-transition: all 0.7s ease; -o-transition: all 0.7s ease; transition: all 0.7s ease;}
#header .header_box {position:relative;width:1200px;margin:0 auto;padding-top:35px;}
#header .header_box:after{content:'';height:0;font-size:0;visibility:hidden;display:block;clear:both;}
#header .logo {position: absolute;left:0;top:35px;display:block;width:260px;font-size: 0;padding-top: 16px;z-index: 9000;}
#header .logo a{display: block;width:226px;height:29px;}
.util_right {position:absolute; right:0;top:0;}
.util_right ul {display:block;float:left;margin-right: 13px;}
.util_right li {display: inline-block;}
.util_right li a {position: relative;display: block;text-align: center;color: #fff;padding:0 9px;line-height: 35px;}
.util_right li a:after {content:'';position: absolute;left: 0;top:50%;width: 1px;height: 10px;margin-top: -5px;background: #fff;}
.util_right li:first-child a:after {background:none !important;}
.util_right li:last-child a {padding-right:0;}
.top_search {position: relative;float: left;width: 180px;padding-right: 32px;height: 35px;border: 1px solid #dbdbdb;}
.top_search span {display: block;}
.top_search span .text {border: 0;height: 33px;font-size: 12px;width: 100%;}
.top_search button {position: absolute;right: 0;top:0;width: 32px;height: 33px;text-align: center;border: 0;color: #0b4d99;background: #fff;line-height: 0;font-size: 18px;}
.go_sitemap {float: right;width: 34px;height: 35px;line-height: 35px;color: #fff;text-align: center;background: #025cad;}

/*gnb*/
